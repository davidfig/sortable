{
  "name": "yy-sortable",
  "version": "0.1.0",
  "description": "Vanilla drag-and-drop sortable list(s)",
  "main": "dist/sortable.js",
  "scripts": {
    "test": "./node_modules/.bin/budo docs/code.js:index.js --dir docs --live",
    "build-demo": "./node_modules/.bin/browserify docs/code.js -o docs/index.js",
    "docs": "./node_modules/.bin/jsdoc -c .jsdoc.json",
    "transpile": "./node_modules/.bin/babel src/sortable.js --out-dir dist --source-maps inline",
    "prepublishOnly": "npm run transpile && npm run build-demo && npm run docs"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "eventemitter3": "^3.0.1"
  },
  "devDependencies": {
    "fork-me-github": "^1.0.3",
    "yy-jsdoc-template": "^1.3.0",
    "highlight.js": "^9.12.0",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "browserify": "^16.1.1",
    "budo": "^11.2.0",
    "jsdoc": "^3.5.5"
  }
}
